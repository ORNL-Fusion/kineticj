Cell[BoxData[
 RowBox[{"CCFD", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"CC", "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"x_", "'"}], "[", "r", "]"}], "\[Rule]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"x", "[", 
             RowBox[{"j", "-", "1"}], "]"}]}], "+", 
           RowBox[{"0", 
            RowBox[{"x", "[", "j", "]"}]}], "+", 
           RowBox[{"x", "[", 
            RowBox[{"j", "+", "1"}], "]"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"2", " ", "h"}], ")"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"x_", "''"}], "[", "r", "]"}], "\[Rule]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"+", 
            RowBox[{"x", "[", 
             RowBox[{"j", "-", "1"}], "]"}]}], "-", 
           RowBox[{"2", 
            RowBox[{"x", "[", "j", "]"}]}], "+", 
           RowBox[{"x", "[", 
            RowBox[{"j", "+", "1"}], "]"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"h", "^", "2"}], ")"}]}]}]}], "}"}]}], "/.", 
    RowBox[{
     RowBox[{"x_", "[", "r", "]"}], "\[Rule]", 
     RowBox[{"x", "[", "j", "]"}]}]}], "//", "FullSimplify"}]}]], "Input",

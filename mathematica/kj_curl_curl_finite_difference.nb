Cell["\<\
Setup an electric field with appropriate phi and z variations and take the \
curl curl in Cylindrical coordinates.\
\>", "Text",
 CellChangeTimes->{{3.723469999012499*^9, 3.7234700411236*^9}, {
  3.7234700893637753`*^9, 3.723470094259625*^9}, {3.723470463285513*^9, 
  3.723470484667472*^9}},ExpressionUUID->"da484b14-cccf-4077-b0e1-\
4971efa40af6"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"E1", "[", 
   RowBox[{"r_", ",", "t_", ",", "z_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Er", "[", "r", "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "nPhi", " ", "t"}], "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "kz", " ", "z"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"E2", "[", 
   RowBox[{"r_", ",", "t_", ",", "z_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Et", "[", "r", "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "nPhi", " ", "t"}], "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "kz", " ", "z"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"E3", "[", 
   RowBox[{"r_", ",", "t_", ",", "z_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Ez", "[", "r", "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "nPhi", " ", "t"}], "]"}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{"i", " ", "kz", " ", "z"}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"CC", ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"Curl", "[", 
     RowBox[{
      RowBox[{"Curl", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"E1", "[", 
           RowBox[{"r", ",", "t", ",", "z"}], "]"}], ",", 
          RowBox[{"E2", "[", 
           RowBox[{"r", ",", "t", ",", "z"}], "]"}], ",", 
          RowBox[{"E3", "[", 
           RowBox[{"r", ",", "t", ",", "z"}], "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"r", ",", "t", ",", "z"}], "}"}], ",", 
        "\"\<Cylindrical\>\""}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"r", ",", "t", ",", "z"}], "}"}], ",", "\"\<Cylindrical\>\""}],
      "]"}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{"i", " ", "nPhi", " ", "t"}], "]"}], "*", 
      RowBox[{"Exp", "[", 
       RowBox[{"i", " ", "kz", " ", "z"}], "]"}]}], ")"}]}], "//", 
   "FullSimplify"}]}]}], "Input",
 CellChangeTimes->{{3.723467261209569*^9, 3.7234674487132387`*^9}, {
   3.723467549385688*^9, 3.723467569264669*^9}, {3.723467878267599*^9, 
